{
  "module_id": "nicolas-viam:sample-apps-module",
  "visibility": "public",
  "description": "This module tests a couple of Viam Apps.",
  "url": "https://github.com/viamrobotics/sample-viam-apps-module",
  "applications": [
    {
      "name": "redbotname",
      "type": "single_machine",
      "entrypoint": "module/apps/red/dist/index.html"
    },
    {
      "name": "bluebotname",
      "type": "single_machine",
      "entrypoint": "module/apps/blue/dist/index.html"
    }
  ],
  "build": {
    "setup": "./setup_node.sh",
    "build": "make build",
    "path": "bin/module.tar.gz"
  }
}
